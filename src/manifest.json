{
    "name": "Seekwords",
    "description" : "Highlight words in page, using aho corasick algorithm.",
    "version": "1.0",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "seekwords.png"
    },
    "manifest_version": 2,
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Open seekwords"
      }
    },
    "permissions": [
      "storage",
      "https://*/",
      "http://*/"
    ]
  }